{
  "name": "ionic-hello-world",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "watch": "ionic-app-scripts watch",
    "build": "ionic-app-scripts build --dev",
    "build:prod": "ionic-app-scripts build --prod",
    "serve": "ionic-app-scripts serve --lab",
    "emulate:android": "npm run build && ionic run android",
    "emulate:ios": "ionic emulate ios --livereload",
    "deploy": "npm run build && ionic run android",
    "resources": "ionic resources",
    "publish:apk": "npm run ionic:release:build && npm run publish:sign:apk && npm run publish:zipalign",
    "postpublish:apk": "npm run install:apk",
    "ionic:release:build": "npm run build:prod && ionic build android --release",
    "publish:sign:apk": "jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore /Users/paulpatarinski/Desktop/privateKey/my-release-key.keystore /Users/paulpatarinski/WIP/Personal/simpleToDo/platforms/android/build/outputs/apk/android-release-unsigned.apk -storepass password alias_name",
    "publish:zipalign": "zipalign -v 4 platforms/android/build/outputs/apk/android-release-unsigned.apk apps/todo.apk",
    "install:apk": "adb install apps/todo.apk",
    "test:android": "cd ui_tests && npm run test:android",
    "screenshots": "cd ui_tests && npm run screenshots && cd .. && npm run screenshots:frame",
    "screenshots:frame" : "cd fastlane/metadata/android/en-US/images && fastlane frameit",
    "copy": "cd ui_tests && npm run copy"
  },
  "config": {
    "ionic_webpack": "./scripts/config/webpack.config.js",
    "ionic_sass": "./scripts/config/sass.config.js"
  },
  "dependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/compiler-cli": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "2.2.1",
    "@angular/http": "2.2.1",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/platform-server": "2.2.1",
    "@ionic/storage": "1.1.7",
    "angular2-moment": "^1.1.0",
    "ionic-angular": "2.0.0-rc.5",
    "ionic-native": "2.2.11",
    "ionicons": "3.0.0",
    "lodash": "^4.17.4",
    "raven-js": "^3.9.1",
    "rxjs": "5.0.0-beta.12",
    "sw-toolbox": "3.4.0",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "@ionic/app-scripts": "1.0.0",
    "copyfiles": "^1.2.0",
    "typescript": "2.0.9"
  },
  "cordovaPlugins": [
    "cordova-plugin-statusbar",
    "cordova-plugin-device",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-console",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": [
    "ios",
    {
      "platform": "ios",
      "version": "",
      "locator": "ios"
    }
  ],
  "description": "simpleToDo: An Ionic project"
}
